ALTER TABLE Alunos
ADD COLUMN Favoritos VARCHAR (255);

UPDATE Alunos
SET Favoritos = CASE
    WHEN ID = 13 THEN 'SIM'
    WHEN ID = 29 THEN 'SIM'
    WHEN ID = 10 THEN 'SIM'
    WHEN ID = 30 THEN 'SIM'
    WHEN ID = 11 THEN 'SIM'
    WHEN ID = 14 THEN 'SIM' 
    WHEN ID = 31 THEN 'SIM'
    WHEN ID = 28 THEN 'SIM'
    WHEN ID = 19 THEN 'SIM'
    WHEN ID = 12 THEN 'SIM'
END;
